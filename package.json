{
  "name": "engx",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "npx react-native run-ios",
    "start": "react-native start",
    "set:env:dev": "node ./scripts/set-env.js --env dev",
    "set:env:qa": "node ./scripts/set-env.js --env qa",
    "set:env:prod": "node ./scripts/set-env.js --env prod",
    "run:android:dev": "npm run set:env:dev && npm run android --variant dev",
    "run:android:qa": "npm run set:env:qa && npm run android --variant qa",
    "run:android:prod": "npm run set:env:prod && npm run android --variant prod",
    "run:ios:dev": "yarn set:env:dev && yarn ios --configuration Dev",
    "run:ios:qa": "yarn set:env:qa && yarn ios --configuration Qa",
    "run:ios:prod": "yarn set:env:prod && yarn ios --configuration Prod",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "npx eslint . --ext .ts,.tsx --fix",
    "debug": "open 'rndebugger://set-debugger-loc?host=localhost&port=8081'",
    "redux-devtools": "npx redux-devtools --open=electron --hostname=localhost --port=8000",
    "serve": "npm run redux-devtools && npm run ios",
    "pod": "cd ios && pod install",
    "unpod": "cd ios && pod deintegrate",
    "reset-cache": "npm start -- --reset-cache",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "clean": "react-native clean-project",
    "clean:auto": "react-native clean-project-auto",
    "e2e:build": "npx detox build -c ios",
    "e2e:test": "npx detox test -c ios --testNamePattern \"^((?!:ios:).)*$\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.15.17",
    "@react-native-community/netinfo": "^7.1.7",
    "@react-navigation/drawer": "^6.1.8",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/native-stack": "^6.2.5",
    "@reduxjs/toolkit": "^1.7.0",
    "@types/react-redux": "^7.1.20",
    "appcenter": "4.4.3",
    "appcenter-analytics": "4.4.3",
    "appcenter-crashes": "4.4.3",
    "detox-cli": "^19.0.0",
    "eslint-plugin-react-native": "^4.0.0",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-native": "0.66.3",
    "react-native-camera": "^4.2.1",
    "react-native-dotenv": "^3.3.1",
    "react-native-elements": "^3.4.2",
    "react-native-geolocation-service": "^5.3.0-beta.4",
    "react-native-gesture-handler": "^2.1.0",
    "react-native-image-picker": "^4.7.3",
    "react-native-maps": "0.30.1",
    "react-native-modal": "^13.0.0",
    "react-native-paper": "^4.11.2",
    "react-native-reanimated": "^2.3.1",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.1",
    "react-native-share": "^7.3.5",
    "react-native-snap-carousel": "^3.9.1",
    "react-native-vector-icons": "^9.0.0",
    "react-redux": "^7.2.6",
    "redux-devtools-extension": "^2.13.9",
    "remote-redux-devtools": "^0.5.16"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@redux-devtools/cli": "^1.0.0-10",
    "@storybook/addon-actions": "^5.3",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.25",
    "@storybook/react-native-server": "^5.3.23",
    "@testing-library/jest-native": "^4.0.4",
    "@testing-library/react-native": "^9.0.0",
    "@types/jest": "^26.0.23",
    "@types/react-native": "^0.66.4",
    "@types/react-native-snap-carousel": "^3.8.5",
    "@types/react-test-renderer": "^17.0.1",
    "@types/remote-redux-devtools": "^0.5.5",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.3",
    "detox": "^19.4.5",
    "eslint": "^7.14.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.21.2",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-detox": "^1.0.0",
    "flowtype": "^2.0.0",
    "husky": "^7.0.0",
    "install": "^0.13.0",
    "jest": "^26.6.3",
    "jest-circus": "^27.5.1",
    "metro-react-native-babel-preset": "^0.66.2",
    "npm": "^8.5.3",
    "react-dom": "17.0.2",
    "react-native-clean-project": "^4.0.1",
    "react-native-storybook-loader": "^2.0.5",
    "react-test-renderer": "^17.0.2",
    "ts-jest": "^26.5.5",
    "typescript": "^4.4.4",
    "yargs": "^17.3.1"
  },
  "resolutions": {
    "@types/react": "^17"
  }
}
